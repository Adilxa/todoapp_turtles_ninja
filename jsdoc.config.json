{
  "source": {
    "include": ["src"],
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "node_modules/better-docs/component",
    "node_modules/better-docs/category"
  ],
  "opts": {
    "destination": "docs",
    "recurse": true,
    "readme": "README.md"
  },
  "templates": {
    "better-docs": {
      "name": "Моя React документация"
    }
  }
}